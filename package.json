{
  "name": "nushu-vscode-theme",
  "displayName": "Nüshu",
  "description": "A VSCode theme that will continue to fade with time",
  "version": "0.4.12",
  "scripts": {
    "release:major": "vsce publish major && git push",
    "release:minor": "vsce publish minor && git push",
    "release:patch": "vsce publish patch && git push",
    "start": "nodemon --watch src src/index.js",
    "build": "node src/index.js"
  },
  "icon": "images/icon.png",
  "publisher": "wheredoesyourmindgo",
  "galleryBanner": {
    "color": "#1b1f23",
    "theme": "dark"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wheredoesyourmindgo/nushu-vscode-theme"
  },
  "bugs": {
    "url": "https://github.com/wheredoesyourmindgo/nushu-vscode-theme/issues"
  },
  "engines": {
    "vscode": "^1.43.0"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "theme",
    "github",
    "light",
    "primer",
    "paper",
    "women",
    "pen paper coffee",
    "parchment"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Nüshu",
        "uiTheme": "vs",
        "path": "./themes/light.json"
      }
    ]
  },
  "license": "MIT",
  "__metadata": {
    "publisherDisplayName": "wheredoesyourmindgo"
  },
  "devDependencies": {
    "@primer/primitives": "^2.0.1",
    "color": "^3.1.2",
    "nodemon": "^2.0.3"
  }
}
